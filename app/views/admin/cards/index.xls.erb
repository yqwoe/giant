<Row>
  <Cell><Data ss:Type="String">卡号</Data></Cell>
  <Cell><Data ss:Type="String">是否激活</Data></Cell>
  <Cell><Data ss:Type="String">购买用户</Data></Cell>
  <Cell><Data ss:Type="String">车牌</Data></Cell>
  <Cell><Data ss:Type="String">激活日期</Data></Cell>
</Row>
<% @cards.each do |card| %>
  <Row>
    <Cell><Data ss:Type="String"><%= card.cid %></Data></Cell>
    <Cell><Data ss:Type="String"><%= card.is_actived? %></Data></Cell>
    <Cell><Data ss:Type="String"><%= card&.car&.user&.name || card&.car&.user&.mobile %></Data></Cell>
    <Cell><Data ss:Type="String"><%= card&.car&.licensed_id  %></Data></Cell>
    <Cell><Data ss:Type="String"><%= card.actived_at&.strftime('%Y-%m-%d %H:%M') %></Data></Cell>
  </Row>
<% end %>
